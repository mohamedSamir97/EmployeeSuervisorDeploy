{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users/(.*)",
        "dest": "/api/users/$1"
      },
      {
        "src": "/api/employees/(.*)",
        "dest": "/api/employees/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/index.html"
    }
    ]
  }
